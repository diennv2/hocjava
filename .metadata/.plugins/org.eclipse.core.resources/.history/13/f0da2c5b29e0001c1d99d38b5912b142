public class HocComment {
    /**
     * Đây là hàm giải ptr bậc 1
     * @param a - hệ số a
     * @param b - hệ số b
     */
    public static void ptb1(double a, double b) {
        
    }
    public static void main(String[] args) {
        // Đây câu lệnh in màn hình
        System.out.println("Helo tui là Điển lè");
        /*
         * Đây là câu lệnh xuất ra màn hình có sử dụng ký tự đặc biệt
         * \n Là xuống dòng
         * \t là dấu tab
         * \" là trích dẫn
         */
        System.out.println("\nHello Obama,\tHello Putin,\"Hello Elon Muck\"");
    }
}
